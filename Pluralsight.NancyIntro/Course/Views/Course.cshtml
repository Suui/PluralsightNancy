@inherits Nancy.ViewEngines.Razor.NancyRazorViewBase<Pluralsight.NancyIntro.Course.Course>

<!DOCTYPE html>
<html>
<head>
    <title></title>
	<meta charset="utf-8" />
</head>
<body>
    <div class="container">
        <h1>Course details for @Model.Name (@Model.Author)</h1>
        <ul>
            @foreach (var module in Model.Modules)
            {
                <li>@module.Topic</li>
            }
        </ul>
        
    </div>
</body>
</html>
